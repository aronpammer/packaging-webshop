{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission",
    "description": "Contact form submissions from website"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "subject": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["new", "in-progress", "resolved", "closed"],
      "default": "new"
    },
    "priority": {
      "type": "enumeration",
      "enum": ["low", "medium", "high", "urgent"],
      "default": "medium"
    },
    "ipAddress": {
      "type": "string",
      "required": false
    },
    "userAgent": {
      "type": "text",
      "required": false
    },
    "notes": {
      "type": "text",
      "required": false
    }
  }
}